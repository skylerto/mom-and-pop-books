<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>Cart</filter-name>
		<display-name>Cart</display-name>
		<description></description>
		<filter-class>filters.Cart</filter-class>
	</filter>
	<filter>
		<filter-name>CheckoutFilter</filter-name>
		<display-name>CheckoutFilter</display-name>
		<description></description>
		<filter-class>filters.CheckoutFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>Cart</filter-name>
		<url-pattern>/Cart</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>CheckoutFilter</filter-name>
		<url-pattern>/CheckoutFilter</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>Soap</servlet-name>
		<display-name>Soap</display-name>
		<description></description>
		<servlet-class>controllers.Soap</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CartController</servlet-name>
		<display-name>CartController</display-name>
		<description></description>
		<servlet-class>controllers.CartController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Soap</servlet-name>
		<url-pattern>/Soap</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>default</servlet-name>
		<url-pattern>*.js</url-pattern>
		<url-pattern>*.css</url-pattern>
		<url-pattern>*.html</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CartController</servlet-name>
		<url-pattern>/CartController</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>unlikelyFileName.xml</welcome-file>
	</welcome-file-list>
	<security-constraint>
		<web-resource-collection>
			<web-resource-name>securedapp</web-resource-name>
			<url-pattern>/*</url-pattern>
		</web-resource-collection>
		<user-data-constraint>
			<transport-guarantee>CONFIDENTIAL</transport-guarantee>
		</user-data-constraint>
	</security-constraint>
</web-app>
